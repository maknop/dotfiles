# Git
alias ga="git add"
alias gc="git commit -S -m"
alias gp="git push"
alias gs="git status"
alias gd="git diff"

# Vim
alias v="nvim"

# Tmux
alias ts="tmux new -s"
alias ta="tmux attach -t"
alias tk="tmux kill-session -t"
alias tka="tmux kill-session -a"
alias tl="tmux ls"
alias tr="tmux rename-window -t"

# Kubectl
alias k="kubectl"

# Docker
alias d="docker"
alias di="docker images"
alias dps="docker ps"
alias dpsa="docker ps -a"

# Podman
alias p="podman"
alias pi="podman images"
alias pps="podman ps"
alias ppsa="podman ps -a"
alias ppsf="podman ps -f"
alias ppsfa="podman ps -f -a"
